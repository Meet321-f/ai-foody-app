{
  "expo": {
    "name": "foody",
    "slug": "foody",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "light",
    "userInterfaceStyle": "automatic",

    "splash": {
      "image": "./assets/images/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0D0D0D"
      },
      "package": "com.anonymous.foody"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },

    "plugins": ["expo-router"],

    "experiments": {
      "typedRoutes": true
    }
  }
}
